{
    "name": "kyc-mfe",
    "version": "1.0.0",
    "description": "KYC Module Federation Remote",
    "scripts": {
        "start": "npm run lint && webpack serve --mode development",
        "start:staging": "npm run lint && webpack serve --mode production --env staging",
        "start:prod": "webpack serve --mode production --env production",
        "build": "webpack --mode production --env production",
        "build:staging": "webpack --mode production --env staging",
        "build:dev": "webpack --mode development",
        "dev": "webpack serve --mode development",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
        "lint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx --max-warnings 0",
        "prepare": "husky"
    },
    "peerDependencies": {
        "react": "^18.2.0 || ^19.0.0",
        "react-dom": "^18.2.0 || ^19.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.0",
        "@babel/preset-env": "^7.23.0",
        "@babel/preset-react": "^7.22.0",
        "@babel/preset-typescript": "^7.23.0",
        "@eslint/js": "^9.39.2",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "autoprefixer": "^10.4.16",
        "axios": "^1.13.2",
        "babel-loader": "^9.1.3",
        "css-loader": "^6.8.1",
        "dotenv-webpack": "^8.1.1",
        "eslint": "^9.39.2",
        "fork-ts-checker-webpack-plugin": "^9.1.0",
        "globals": "^17.0.0",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^9.1.7",
        "postcss": "^8.4.31",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.8.0",
        "react-router-dom": "^7.11.0",
        "style-loader": "^3.3.3",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.9.3",
        "webpack": "^5.104.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.2.3"
    }
}
